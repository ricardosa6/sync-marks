{
  "name": "Sync Marks",
  "version": "1.0.0",
  "manifest_version": 3,
  "default_locale": "en",
  "permissions": ["bookmarks", "storage"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "serviceWorker.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
